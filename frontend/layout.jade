//- TODO: Remove from git
doctype html
html
	head
		!= newrelic.getBrowserTimingHeader()
		title
			block title
				| #{config.appName}
		link(rel='stylesheet' href='//maxcdn.bootstrapcdn.com/bootswatch/3.3.0/paper/bootstrap.min.css')
		link(rel='stylesheet' href='/static/css/main.css')
		block head-scripts
		meta(name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no")
	body
		.page-wrapper
			.header
				nav.navbar.navbar-inverse.square
					.container
						.navbar-header
							a.text-uppercase.navbar-brand(href="#" style="padding: 0")
								img(src="/static/img/logo-white.png", alt="" style="height: 64px")
						div(ng-include='\'templates/profiles/list-tmpl\'')
			.content
				.container
					block content
			.footer
				.block-center.block-middle.text-center.text-uppercase
					span copyright Â©&nbsp;
					span #{(new Date(Date.now())).getFullYear() + " " + config.appName}
					small &nbsp;(#{package.version})
		block body-scripts